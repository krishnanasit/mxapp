@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap');

*{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
	outline: none;
}

:root{
	--primary:  #20c997 ;
	--secondary: #6c757d;
	--white: #ffffff;
	--black: #000000;
	--textdark: #252b33;
	--textlight:#8e9a9d;
	--pad15: 15px;
	--pad100: 100px;
	
}

.container{
	margin: auto;
}


.top i{
	width: 40px;
	height: 40px;
	font-size: 14px;
	display: block;
	text-align: center;
	line-height: 40px;
	border-radius: 4px 0px 0px 4px;
	background-color: rgba(0,0,0,0.2);
	text-decoration: none;
	position: fixed;
	right: 0px;
	bottom: 380px;
	box-shadow :0px 0px 3px 1px rgb(0,0,0.1);
	color: var(--white);
	font-size: 21px;
	background-color: rgb(51, 51, 51);
}


.backtotop i{
	width: 36px;
	height: 36px;
	font-size: 14px;
	display: block;
	text-align: center;
	line-height: 36px;
	border-radius: 50%;
	background-color: rgba(0,0,0,0.2);
	text-decoration: none;
	position: fixed;
	right: 10px;
	bottom: 10px;
    box-shadow: 0px 5px 15px rgb(0 0 0 / 15%);
	transition: all 0.5s;
	color: var(--white);
	font-size: 16px;
	background-color: rgba(0, 0, 0, 0.2)
}

.backtotop i:hover{
		background-color:var(--primary);
}

.backtotop:hover{
	background-color:var(--primary);
}
body{
	font-family: 'Poppins', sans-serif;
	padding-top: 60px;
}

img{
	max-width: 100%;
}

.row{
	display: flex;
	flex-wrap: wrap;
}

.pad100{
	padding: var(--pad100) var(--pad15);
}

.justify-content-end{
	justify-content: flex-end;
}

.justify-content-between{
	justify-content: space-between;
}

.justify-content-center{
	justify-content: center;
}

.align-self-center{
	align-self: center;
}

.d-block{
	display: block;
}

.d-inline-block{
	display: inline-block;
}


ul, ol{
	list-style: none;
}


.text-align{
	text-align: center;
}

.text-right{
	text-align: right;
}

.text-justify{
	text-align: justify;
}

header{
	padding: var(--pad15);
	position: fixed;
	width: 100%;
	top: 0;
	background-color: white;
	z-index: 9999;
	box-shadow: 0 0 10px rgba(0,0,0,0.1);
}

header ul li a{
	font-family: var(--font);
	text-decoration: none;
	padding: 0px 12px;
	font-weight: 600;
	color: var(--textdark);
	transition: all 0.4s;
	font-size: 16px;
	position: relative;
}

header ul li a:hover{
	color: var(--primary);
}

header ul li:last-child a{
	padding: 7px 20px;
	background-color: var(--primary);
	color: var(--white);
	border-radius: 4px;
	transition: all 0.4s;
	margin-left: 10px;
}

header ul li:last-child a:hover{
	background-color: var(--white);
	color: var(--primary);
	box-shadow: 0 0 7px rgba(0, 0, 0, 0.1);
}










